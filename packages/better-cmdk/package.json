{
  "name": "better-cmdk",
  "version": "0.0.21",
  "type": "module",
  "main": "./index.ts",
  "module": "./index.ts",
  "types": "./index.ts",
  "exports": {
    ".": {
      "types": "./index.ts",
      "style": "./index.css",
      "import": "./index.ts"
    },
    "./utils": {
      "import": "./lib/utils.ts",
      "types": "./lib/utils.ts"
    },
    "./index.css": "./index.css",
    "./styles.css": "./dist/styles.css"
  },
  "style": "./index.css",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist",
    "AGENTS.md",
    "index.css"
  ],
  "scripts": {
    "build:css": "tailwindcss -i ./styles/index.css -o ./dist/styles.css --minify && bun run scripts/sanitize-library-css.ts",
    "build:prompts": "bun run scripts/build-prompts.ts",
    "build": "bun run --bun tsdown && bun run build:css && bun run build:prompts",
    "build:publish": "bun run --bun tsdown --publish && bun run build:css && bun run build:prompts"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@biomejs/biome": "^2.3.14",
    "@types/bun": "latest",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "tailwindcss": "^4.1.18",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19",
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-sdk/react": "^3.0.0",
    "@json-render/core": "^0.2.0",
    "@json-render/react": "^0.2.0",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-compose-refs": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-id": "^1.1.0",
    "@radix-ui/react-primitive": "^2.0.2",
    "@sentry/browser": "^9.0.0",
    "ai": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "motion": "^12.0.0",
    "radix-ui": "^1.4.3",
    "streamdown": "^2.1.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^3.24.0"
  }
}
